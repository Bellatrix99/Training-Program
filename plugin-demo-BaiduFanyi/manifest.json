{
    "manifest_version": 2,
    "name": "划译工具-百度翻译",
    "description": "划译工具-百度翻译",
    "version": "1.0.0",
    "icons": {
        "16": "images/translater.png",
        "32": "images/translater.png",
        "48": "images/translater.png",
        "128": "images/translater.png"
